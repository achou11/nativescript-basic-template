{
	"name": "nativescript-basic-template",
	"version": "1.0.0",
	"private": true,
	"main": "src/app.ts",
	"packageManager": "pnpm@10.1.0",
	"scripts": {
		"android:build": "ns build android",
		"android:run": "ns run android",
		"ios:build": "ns build ios",
		"ios:run": "ns run ios",
		"lint:format": "prettier --check --cache .",
		"lint:eslint": "eslint --cache .",
		"lint:types": "tsc --noEmit",
		"lint": "pnpm run \"/lint:.*/\"",
		"prepare": "husky",
		"start": "ns start"
	},
	"dependencies": {
		"@nativescript/core": "8.8.6",
		"@nativescript/theme": "3.1.0"
	},
	"devDependencies": {
		"@eslint/compat": "1.2.6",
		"@eslint/js": "9.19.0",
		"@ianvs/prettier-plugin-sort-imports": "4.4.1",
		"@nativescript/android": "8.8.6",
		"@nativescript/ios": "8.8.2",
		"@nativescript/types": "8.8.0",
		"@nativescript/webpack": "5.0.22",
		"@prettier/plugin-xml": "3.4.1",
		"@types/node": "22.13.0",
		"eslint": "9.19.0",
		"globals": "15.14.0",
		"husky": "9.1.7",
		"nano-staged": "0.8.0",
		"nativescript": "8.8.3",
		"prettier": "3.4.2",
		"prettier-plugin-jsdoc": "1.3.2",
		"stringify-package": "1.0.1",
		"typescript": "5.7.3",
		"typescript-eslint": "8.22.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@nativescript/core",
			"nativescript"
		]
	}
}
