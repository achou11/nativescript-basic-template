{
	"name": "nativescript-basic-template",
	"version": "1.0.0",
	"private": true,
	"main": "src/app.ts",
	"packageManager": "pnpm@10.13.1",
	"scripts": {
		"android:build": "ns build android",
		"android:run": "ns run android",
		"ios:build": "ns build ios",
		"ios:run": "ns run ios",
		"check:format": "prettier --check --cache .",
		"check:eslint": "eslint --cache .",
		"check:types": "tsc --noEmit",
		"check": "pnpm run \"/check:.*/\"",
		"prepare": "husky",
		"start": "ns start"
	},
	"dependencies": {
		"@nativescript/core": "8.9.7",
		"@nativescript/theme": "3.1.0"
	},
	"devDependencies": {
		"@eslint/compat": "1.3.1",
		"@eslint/js": "9.32.0",
		"@ianvs/prettier-plugin-sort-imports": "4.5.1",
		"@nativescript/android": "8.9.1",
		"@nativescript/ios": "8.9.2",
		"@nativescript/types": "8.9.1",
		"@nativescript/webpack": "5.0.24",
		"@prettier/plugin-xml": "3.4.2",
		"@types/node": "22.16.5",
		"eslint": "9.32.0",
		"globals": "16.3.0",
		"husky": "9.1.7",
		"nano-staged": "0.8.0",
		"nativescript": "8.9.3",
		"prettier": "3.6.2",
		"prettier-plugin-jsdoc": "1.3.3",
		"typescript": "5.8.3",
		"typescript-eslint": "8.38.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@nativescript/core",
			"nativescript"
		]
	}
}
